{
  "name": "biinhouse",
  "version": "1.0.0",
  "description": "Electron App For BI InHouse.",
  "repository": "https://bisolutions.com.np/",
  "author": "BISolutions <office.bisolutions.com.np>",
  "homepage":"http://bierp.aquaworld.com.np/",
  "main": "app.js",
  "scripts": {
    "postinstall": "install-app-deps",
    "pack": "build --dir",
    "dist": "build",
    "start": "electron app.js"
  },
  "license": "ISC",
  "devDependencies": {
    "electron": "^3.0.2",
    "electron-builder": "^20.38.5"
  },
  "build": {
    "appId": "com.aquaworld.app",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
        "target": [
          {
            "target": "nsis",
            "arch": [
              "x64",
              "ia32"
            ]
          }
        ]
      }
  }

}
